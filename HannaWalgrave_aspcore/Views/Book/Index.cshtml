
@model BookListViewModel
    @{ 
        ViewData["Title"] = "Overzicht Boeken";
    }

<div class="row">
    <div class="col-md-12">
        <ul>
            @foreach(var book in Model.Books)
            {
                <li>@book.Title</li>
                <li>@book.Author</li>
                <li>@book.ISBN</li>
                <li>@book.CreatedOn</li>
            
            }
        </ul>
    </div>
</div>